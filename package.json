{
  "name": "shop-apotheke-backend-challenge",
  "version": "1.0.0",
  "description": "backend application for discovering popular repositories on GitHub",
  "repository": "https://github.com/ned-aq/sa-backend-challenge.git",
  "scripts": {
    "build": "rimraf ./dist && tsc -p tsconfig.json",
    "debug": "node --inspect -r ts-node/register src/server",
    "lint": "eslint \"{src,test}/**/*.ts\" --max-warnings 0",
    "start": "node -r ts-node/register --inspect src/server",
    "start:dev": "rimraf ./dist && nodemon",
    "start:prod": "node dist/server",
    "test": "npm run build && mocha --colors --bail --exit --timeout 60000 --recursive dist/test/unit",
    "coverage": "npm run build && nyc --reporter=lcov --reporter=text mocha --colors --bail --exit --timeout 60000 --recursive dist/test/unit"
  },
  "author": "Nedal Alaqraa",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.3.2",
    "body-parser": "^1.20.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "class-validator-jsonschema": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.10.1",
    "routing-controllers-openapi": "^4.0.0",
    "swagger-ui-express": "^4.6.0",
    "typedi": "^0.10.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/chai": "^4.3.4",
    "@types/express": "^4.17.17",
    "@types/mocha": "^10.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.19",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "chai": "^4.3.7",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "mocha": "^10.2.0",
    "nock": "^13.3.0",
    "nodemon": "^2.0.20",
    "nyc": "^15.1.0",
    "prettier": "^2.8.3",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}